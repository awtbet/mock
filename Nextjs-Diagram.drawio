<?xml version="1.0" encoding="UTF-8"?>
<mxGraphएमएक्सग्राफ (mxGraph)>
    <defaults as="default" id="defaultStyle">
        <vertex style="shape=participant;whiteSpace=wrap;verticalLabelAlignment=middle">
            <font style="fontSize=12"/>
        </vertex>
        <edge style="strokeColor=#7991B3;strokeWidth=2;fontColor=#000000;fontSize=12"/>
    </defaults>
    <graph autofit="1" id="0">
        <vertex id="1" value="PC">
            <port name="Open Web Portal" spot="bottom"/>
        </vertex>
        <vertex id="2" value="Web Portal">
            <port name="Receive Purchase Request" spot="top"/>
            <port name="Send Confirmation &amp; Credentials" spot="bottom"/>
        </vertex>
        <vertex id="3" value="Internet Service Provider (ISP)">
            <port name="Process Purchase" spot="top"/>
            <port name="Activate Internet Service" spot="bottom"/>
        </vertex>
        <vertex id="4" value="Internet">
            <port name="Connect" spot="right"/>
        </vertex>
        <edge id="5" source="1:Open Web Portal" target="2:Receive Purchase Request">
            <label x="0.5" y="0.5" style="verticalAlign=middle">Open Web Portal (Buy Internet Plan)</label>
        </edge>
        <edge id="6" source="2:Send Confirmation &amp; Credentials" target="3:Process Purchase">
            <label x="0.5" y="0.5" style="verticalAlign=middle">Send Confirmation &amp; Payment Details</label>
        </edge>
        <edge id="7" source="3:Activate Internet Service" target="2:Send Confirmation &amp; Credentials">
            <label x="0.5" y="0.5" style="verticalAlign=middle">Activate Internet Service &amp; Send Credentials</label>
        </edge>
        <edge id="8" source="2:Send Confirmation &amp; Credentials" target="1:Open Web Portal">
            <label x="0.5" y="0.5" style="verticalAlign=middle">Receive Confirmation &amp; Login Credentials</label>
        </edge>
        <edge id="9" source="1:Open Web Portal" target="4:Connect">
            <label x="0.5" y="0.5" style="verticalAlign=middle">Connect to Internet</label>
        </edge>
    </graph>
</mxGraphएमएक्सग्राफ (mxGraph)>
